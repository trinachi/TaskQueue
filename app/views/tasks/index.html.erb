<div id="main">
	<p class="notice"><%= notice %></p>
	<p class="alert"><%= alert %></p>
	<%= render 'filter' %>
	<div class="clear"></div>

	<section id="all_tasks">
		<div id="incomplete">
			<h1>Current Tasks</h1>
			<ul id="tasks">
				<%= render 'not_completed'%>
			</ul>
		</div>
		
		<div id="complete">
			<h1>Completed Tasks</h1>
			<ul id="tasks">
				<%= render 'completed'%>
			</ul>
		</div>
		<div class="clear"></div>
	</section>
</div>




